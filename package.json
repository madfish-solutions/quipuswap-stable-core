{
  "name": "quipuswap-stable-core",
  "version": "0.0.1",
  "homepage": "https://github.com/madfish-solutions/quipuswap-stable-core#readme",
  "repository": "github:madfish-solutions/quipuswap-stable-core.git",
  "bugs": "https://github.com/madfish-solutions/quipuswap-stable-core/issues",
  "description": "Token2Token DEX for equal-price tokens based on StableSwap algos on top of Tezos. This package installs dependencies for testing and deployment purposes",
  "author": "Illia Likhoshva <likhoshva@madfish.solutions> (https://www.madfish.solutions/)",
  "private": true,
  "dependencies": {
    "@taquito/signer": "^9.1.1",
    "@taquito/taquito": "^9.1.1",
    "@types/jest": "^27.0.2",
    "create-tezos-smart-contract": "^1.1.11",
    "date-fns": "^2.25.0",
    "jest": "^27.0.6",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "engines": {
    "node": ">=14",
    "yarn": ">=1.22.0"
  },
  "scripts": {
    "start": "npx tezos-sdk -- --help",
    "compile": "yarn compile-contract && yarn compile-lambda-all",
    "clear-build": "rm -rf ./build",
    "recompile": "yarn clear-build && yarn compile-contract && yarn compile-lambda-all",
    "compile-contract": "npx tezos-sdk compile",
    "compile-lambda": "ts-node ./scripts/cli.ts compile-lambda",
    "compile-lambda-all": "yarn compile-lambda-admin && yarn compile-lambda-dex && yarn compile-lambda-permit && yarn compile-lambda-token",
    "compile-lambda-dex": "yarn -s compile-lambda -T Dex -J ./test/lambdas/Dex_lambdas.json -C ./contracts/main/dex.ligo",
    "compile-lambda-token": "yarn -s compile-lambda -T Token -J ./test/lambdas/Token_lambdas.json -C ./contracts/main/dex.ligo",
    "compile-lambda-permit": "yarn -s compile-lambda -T Permit -J ./test/lambdas/Permit_lambdas.json -C ./contracts/main/dex.ligo",
    "compile-lambda-admin": "yarn -s compile-lambda -T Admin -J ./test/lambdas/Admin_lambdas.json -C ./contracts/main/dex.ligo",
    "start-sandbox": "npx tezos-sdk start-sandbox",
    "test": "npx tezos-sdk test",
    "compile-n-test": "yarn compile && yarn test",
    "deploy": "npx tezos-sdk deploy"
  },
  "keywords": [
    "quipuswap",
    "StableSwap",
    "DEX",
    "tezos",
    "blockchain"
  ],
  "devDependencies": {
    "eslint": "^8.2.0",
    "eslint-plugin-jest": "^25.2.4",
    "eslint-plugin-jest-formatting": "^3.0.0"
  }
}
